{
  "name": "recruiter-dashboard",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "json-server --watch mock/db.js --routes mock/routes.json & rollup -c -w",
    "start": "node mock/server.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish": "yarn build && surge public recruiter-dashboard.surge.sh"
  },
  "heroku-run-build-script": false,
  "browserslist": [
    "cover 80% and not last 2 versions and not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/svelte": "^6.1.17",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "faker": "^5.4.0",
    "gravatar": "^1.8.1",
    "postcss": "^8.2.6",
    "rollup": "^2.3.4",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.32.3",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.6.9"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "color": "^3.1.3",
    "d3-interpolate": "^2.0.1",
    "json-server": "^0.16.3",
    "normalize.css": "^8.0.1",
    "numeral": "^2.0.6",
    "perfect-scrollbar": "^1.5.0",
    "progressbar.js": "^1.1.0"
  }
}
